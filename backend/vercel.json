{
  "version": 2,
  "builds": [
    {
      "src": "src/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "COHERE_API_KEY": "@cohere_api_key",
    "QDRANT_URL": "@qdrant_url",
    "QDRANT_API_KEY": "@qdrant_api_key",
    "QDRANT_COLLECTION": "@qdrant_collection",
    "FRONTEND_URL": "@frontend_url",
    "SITEMAP_URL": "@sitemap_url"
  }
}
